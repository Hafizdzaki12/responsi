<!-- Koneksi Database -->

<?php
	$servername = "localhost";
	$username = "root";
	$password = "";
	
	//Membuat koneksi
	$conn = mysqli_connect($servername, $username, $password);
	
	//Memeriksa koneksi
	if (!$conn){ //jika tidak terkoneksi
		die("Connection failed: ". mysqli_connect_error());
	}
	echo "Connected successfully";
?>
